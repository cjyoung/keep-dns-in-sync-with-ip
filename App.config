<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="serviceConfig" type="keep_dns_in_sync_with_ip.ServiceConfigSection, keep-dns-in-sync-with-ip" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <serviceConfig pollFrequencyInMinutes="60">
    <cPanel>
      <username value="username" />
      <password value="password" />
      <cPanelURL value="https://cpanel:2083" />
      <domain value="domain.tld" />
      <DNSNames>
        <DNSName value="dns1.domain.tld" />
        <DNSName value="dns2.domain.tld" />
        <DNSName value="dns3.domain.tld" />
      </DNSNames>
    </cPanel>
  </serviceConfig>
</configuration>